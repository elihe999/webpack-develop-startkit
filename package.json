{
  "name": "webpack-develop-startkit",
  "title": "webpack-develop-startkit",
  "version": "0.0.1",
  "description": "a react start kit for react",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf output/*",
    "compile": "better-npm-run compile",
    "lint": "eslint bin build config server src tests --ext .jsx,.js",
    "lint:fix": "npm run lint -- --fix",
    "start": "better-npm-run start",
    "dev": "better-npm-run dev",
    "test": "better-npm-run test",
    "test:dev": "npm run test -- --watch",
    "deploy": "better-npm-run deploy",
    "deploy:dev": "better-npm-run deploy:dev",
    "deploy:prod": "better-npm-run deploy:prod",
    "deploy:analyze": "better-npm-run deploy:analyze",
    "develop:analyze": "better-npm-run develop:analyze",
    "codecov": "cat coverage/*/lcov.info | codecov",
    "precommit": "npm run lint:fix",
    "prepush": "npm run lint:fix",
    "commitmsg": "validate-commit-msg",
    "commit": "git-cz ",
    "preview": "NODE_ENV=release node ./bin/dev-server.js",
    "dll": "NODE_ENV=production node ./node_modules/webpack/bin/webpack --config config/webpack.dll.lib.js --progress",
    "dll:dev": "NODE_ENV=development node ./node_modules/webpack/bin/webpack --config config/webpack.dll.lib.js --progress"
  },
  "betterScripts": {
    "compile": {
      "command": "node bin/compile",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "dev": {
      "command": "nodemon bin/dev-server --ignore dist --ignore coverage --ignore tests --ignore src",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "deploy": {
      "command": "yarn run lint && yarn run clean && yarn run compile",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "deploy:dev": {
      "command": "yarn run deploy",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "deploy:prod": {
      "command": "yarn run deploy",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "deploy:analyze": {
      "command": "yarn run deploy",
      "env": {
        "NODE_ENV": "production",
        "ANALYZE_ENV": "analyze"
      }
    },
    "develop:analyze": {
      "command": "yarn run deploy",
      "env": {
        "NODE_ENV": "development",
        "ANALYZE_ENV": "analyze"
      }
    },
    "start": {
      "command": "node bin/dev-server",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "test": {
      "command": "node ./node_modules/karma/bin/karma start config/karma.config",
      "env": {
        "NODE_ENV": "test",
        "DEBUG": "app:*"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/taikongfeizhu/webpack-develop-startkit"
  },
  "author": "<huangjian1820@gmail.com> (segmentfault.com/u/abcat)",
  "license": "MIT",
  "keywords": [
    "react",
    "webpack 2.x",
    "babel",
    "webpack2-react-redux-starter"
  ],
